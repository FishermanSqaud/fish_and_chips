{
  "name": "Fish and Chips",
  "version": "1.0",
  "description": "Beware of your web security!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "bookmarks",
    "http://*/*",
    "https://*/*"
  ],
  "background" : {
    "scripts" : [
      "js/background.js"
    ],
    "web_accessible_resources" : [
      "js/popup.js",
      "js/consts.js"
    ],
    "persistent" : false
  },
  "page_action" : {
    "default_popup" : "popup.html",
    "default_icon": {
      "16": "images/fish_and_chips_icon_16.png",
      "32": "images/fish_and_chips_icon_32.png",
      "48": "images/fish_and_chips_icon_48.png",
      "128": "images/fish_and_chips_icon_128.png"
    }
  },
  "icons": {
    "16": "images/fish_and_chips_icon_16.png",
    "32": "images/fish_and_chips_icon_32.png",
    "48": "images/fish_and_chips_icon_48.png",
    "128": "images/fish_and_chips_icon_128.png"
  },
  "manifest_version": 2
}
